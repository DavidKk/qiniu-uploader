{
  "name": "qiniup",
  "version": "0.1.1",
  "description": "Qiniu Uploader",
  "main": "dist/qiniup.js",
  "author": "<David Jones qowera@gmail.com>",
  "license": "MIT",
  "scripts": {
    "yamllint": "yamllint .travis.yml",
    "standard": "standard ./src/**/*.js ./unitest/**/*.spec.js ./*.js --parser babel-eslint",
    "standard:fix": "standard ./src/**/*.js ./unitest/**/*.spec.js ./*.js --parser babel-eslint --fix",
    "lint": "npm run yamllint && npm run standard",
    "lint:fix": "npm run yamllint && npm run standard:fix",
    "unitest": "cross-env UNITEST=1 karma start ./karma.conf.js",
    "unitest:coverage": "rimraf ./coverage && cross-env UNITEST=1 COVERAGE=1 karma start ./karma.conf.js",
    "build": "cross-env PRODUCT=1 webpack --config ./webpack.product.config.babel.js",
    "release": "npm run lint && npm run unitest && npm run build",
    "coveralls": "npm run unitest:coverage && node ./node_modules/.bin/coveralls < ./coverage/lcov.info",
    "sauce": "cross-env SAUCE_LABS=1 npm run unitest",
    "document": "rimraf ./docs && esdoc -c ./esdoc.json",
    "start": ""
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^5.0.0",
    "fs-extra": "^3.0.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.1",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.1",
    "standard": "^10.0.2",
    "urijs": "^1.18.10",
    "webpack": "^2.6.0",
    "webpack-merge": "^4.1.0",
    "yaml-lint": "^0.0.4",
    "async": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "esdoc": "^0.5.2",
    "lodash": "^4.17.4"
  }
}
