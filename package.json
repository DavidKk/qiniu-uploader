{
  "name": "qiniup",
  "version": "0.2.1",
  "description": "Qiniu Uploader",
  "main": "dist/qiniup.js",
  "author": "<David Jones qowera@gmail.com>",
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "scripts": {
    "yamllint": "yamllint .travis.yml",
    "eslint": "eslint --config .eslintrc.js --ext .js --fix './src/**/*.js'",
    "lint": "npm run yamllint && npm run eslint",
    "unitest": "cross-env UNITEST=1 karma start ./karma.conf.js",
    "unitest:coverage": "rimraf ./coverage && cross-env UNITEST=1 COVERAGE=1 karma start ./karma.conf.js",
    "build": "cross-env PRODUCT=1 webpack --config ./webpack.product.config.babel.js",
    "develop": "webpack-dev-server --config ./webpack.develop.config.babel.js --content-base ./dist",
    "release": "npm run lint && npm run unitest && npm run build",
    "codecov": "npm run unitest:coverage && codecov",
    "sauce": "cross-env SAUCE_LABS=1 npm run unitest",
    "docs:esdoc": "rimraf ./gh-pages/esdoc && esdoc -c ./.esdoc.json",
    "docs:used": "rimraf ./gh-pages/usedoc && cpx './README.md' ./documents && cpx './documents/**/*' ./gh-pages/usedocs",
    "docs:used:server": "concurrently \"cpx './README.md' ./documents --watch\" \"docsify serve ./documents\"",
    "docs": "concurrently \"npm run docs:esdoc\" \"npm run docs:used\"",
    "example": "rimraf ./demo && cross-env PRODUCT=1 webpack --config ./webpack.example.config.babel.js",
    "gh-pages": "rimraf gh-pages && concurrently \"npm run docs\" \"npm run example\" ",
    "start": "npm run develop"
  },
  "dependencies": {
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "async": "^2.6.0",
    "autoprefixer": "^8.4.1",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.4",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^0.28.11",
    "docsify-cli": "^4.2.1",
    "eruda": "^1.4.3",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.0.0-alpha.2",
    "eslint-config-standard": "12.0.0-alpha.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "fs-extra": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jqlite": "^0.2.42",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.0.1",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^7.0.1",
    "sinon": "^5.0.7",
    "style-loader": "^0.21.0",
    "urijs": "^1.19.1",
    "webpack": "4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "yaml-lint": "^1.2.3"
  }
}
