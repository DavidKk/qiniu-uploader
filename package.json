{
  "name": "qiniup",
  "version": "0.2.1",
  "description": "Qiniu Uploader",
  "main": "dist/qiniup.js",
  "author": "<David Jones qowera@gmail.com>",
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "scripts": {
    "yamllint": "yamllint .travis.yml",
    "eslint": "eslint --config .eslintrc.js --ext .js --fix './src/**/*.js'",
    "lint": "npm run yamllint && npm run eslint",
    "unitest": "cross-env UNITEST=1 karma start ./karma.conf.js",
    "unitest:coverage": "rimraf ./coverage && cross-env UNITEST=1 COVERAGE=1 karma start ./karma.conf.js",
    "build": "cross-env PRODUCT=1 webpack --config ./webpack.product.config.babel.js",
    "release": "npm run lint && npm run unitest && npm run build",
    "codecov": "npm run unitest:coverage && codecov",
    "sauce": "cross-env SAUCE_LABS=1 npm run unitest",
    "document": "rimraf ./docs && esdoc -c ./.esdoc.json",
    "example": "rimraf ./demo && cross-env PRODUCT=1 webpack --config ./webpack.example.config.babel.js",
    "start": "webpack-dev-server --config ./webpack.develop.config.babel.js --content-base ./dist"
  },
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "async": "^2.4.1",
    "autoprefixer": "^8.0.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bourbon": "^5.0.0",
    "chai": "^4.1.2",
    "cross-env": "^5.0.0",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^0.28.10",
    "esdoc": "^1.0.2",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jqlite": "^0.2.42",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.12",
    "mocha": "^5.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.1.1",
    "pug": "^2.0.0-rc.2",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.4.2",
    "style-loader": "^0.20.2",
    "urijs": "^1.19.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^3.0.0",
    "webpack-merge": "^4.1.2",
    "yaml-lint": "^1.1.0",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3"
  }
}
