{
  "name": "qiniup",
  "version": "0.1.3",
  "description": "Qiniu Uploader",
  "main": "dist/qiniup.js",
  "author": "<David Jones qowera@gmail.com>",
  "license": "MIT",
  "scripts": {
    "yamllint": "yamllint .travis.yml",
    "standard": "standard ./src/**/*.js ./unitest/**/*.spec.js ./*.js --parser babel-eslint",
    "standard:fix": "standard ./src/**/*.js ./unitest/**/*.spec.js ./*.js --parser babel-eslint --fix",
    "lint": "npm run yamllint && npm run standard",
    "lint:fix": "npm run yamllint && npm run standard:fix",
    "unitest": "cross-env UNITEST=1 karma start ./karma.conf.js",
    "unitest:coverage": "rimraf ./coverage && cross-env UNITEST=1 COVERAGE=1 karma start ./karma.conf.js",
    "build": "cross-env PRODUCT=1 webpack --config ./webpack.product.config.babel.js",
    "release": "npm run lint && npm run unitest && npm run build",
    "codecov": "npm run unitest:coverage && codecov",
    "sauce": "cross-env SAUCE_LABS=1 npm run unitest",
    "document": "rimraf ./docs && esdoc -c ./esdoc.json",
    "example": "rimraf ./demo && cross-env PRODUCT=1 webpack --config ./webpack.example.config.babel.js",
    "start": "webpack-dev-server --config ./webpack.develop.config.babel.js --content-base ./dist"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "async": "^2.4.1",
    "autoprefixer": "^7.1.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bourbon": "^4.3.4",
    "chai": "^3.5.0",
    "cross-env": "^5.0.0",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^0.28.4",
    "esdoc": "^0.5.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jqlite": "^0.2.42",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "pug": "^2.0.0-rc.2",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.1",
    "standard": "^10.0.2",
    "style-loader": "^0.18.2",
    "urijs": "^1.18.10",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0",
    "yaml-lint": "^0.0.4"
  }
}
