{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "./src",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "node_modules/jsdoc-babel",
    "plugins/markdown"
  ],
  "babel": {
    "extensions": ["js"]
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}